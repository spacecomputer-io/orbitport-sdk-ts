{
  "name": "@spacecomputer/orbitport-sdk",
  "version": "0.0.1",
  "description": "Official TypeScript SDK for SpaceComputer Orbitport",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest tests/e2e.test.ts",
    "test:unit": "jest tests/unit/",
    "prepublishOnly": "npm run clean && npm run build && npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easonchai/orbitport-sdk.git"
  },
  "keywords": [
    "spacecomputer",
    "orbitport",
    "ctrng",
    "rng",
    "sdk",
    "typescript",
    "random",
    "cryptography"
  ],
  "author": "Eason Chai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/easonchai/orbitport-sdk/issues"
  },
  "homepage": "https://github.com/easonchai/orbitport-sdk#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  }
}
